/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    
    for(var i=0;i<=nums.length;i++)
        {
            for(var j=0;j<=nums.length;j++)
                
             {  
                if (i!=j)
                    {
                    var ans=nums[i]+nums[j];
                    if(ans==target)
                        {   
                            var result=[i,j];
                            return result;     /*if find the correct answer, return immediatley*/
                                
                        }
                    }
             }
    
        }
};
